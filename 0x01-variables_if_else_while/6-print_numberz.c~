#include <stdio.h>
/* Input other headers here */

/**
 * main - Entry Point
 *
 * This program will print numbers 0-9
 * using function "putchar" with a new line.
 *
 * Return: 0 for successful execution
 */
int main(void)
{
	int num;

	for (num = '0'; num <= '9'; num++)
		putchar(0 + num);
		putchar('\n');

	return (0);
}
