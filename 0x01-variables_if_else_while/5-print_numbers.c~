#include <stdio.h>
/* Input other headers here */

/**
 * main - Entry Point
 *
 * This program will print numbers from
 * 0-9 with a new line.
 *
 * Return: 0 for successful execution
 */
int main(void)
{
	int num;

	for (num = 0; num <= 9; num++)
	{
		printf("%d ", num);
	}

	printf("\n");

	return (0);
}
