#include "3-calc.h"

int op_add(int a, int b)
{
	int add;
	add = a + b;
	return (add);
}

int op_sub(int a, int b)
{
	int sub;
	sub = a - b;
	return (sub);
}

int op_mul(int a, int b)
{
	int mul;
	mul = a * b;
	return (mul);
}

int op_div(int a, int b)
{
	int div;
	div = a / b;
	return (div);
}

int op_mod(int a, int b)
{
	int mod;
	mod = a % b;
	return (mod);
}
